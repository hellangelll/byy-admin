{
    "extends": "plugin:vue/strongly-recommended",
    "root": true,
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true
    },
    "plugins": ["vue", "standard"],
    "rules": {
        "no-console": 0,
        "no-new": 0,
        "no-fallthrough": 0,
        "no-unreachable": 0,
        "camelcase": 0,
        "no-unused-vars": 0,
        "prefer-promise-reject-errors": 0,
        "no-undef": 0,
        "no-caller": 0,
        "one-var": 0,
        "vue/no-parsing-error": [2, {
            "x-invalid-end-tag": false
        }],
        "vue/require-default-prop": 0,
        "vue/max-attributes-per-line": 0
    }
}